{
    "name": "dram1008/tpk",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://rod.galaxysss.ru/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-imagine": "*",
        "yiisoft/yii2-smarty": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "*",
        "dram1008/library": "*",
        "mistic100/Bootstrap-Confirmation": "*",
        "lipis/bootstrap-social": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mistic100/Bootstrap-Confirmation",
                "version" : "1.0.0",
                "source": {
                    "url": "https://github.com/mistic100/Bootstrap-Confirmation",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "lipis/bootstrap-social",
                "version": "1.0.1",
                "source": {
                    "url": "https://github.com/lipis/bootstrap-social",
                    "type": "git",
                    "reference": "gh-pages"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dram1008/library",
                "version": "1.1.8",
                "source": {
                    "url": "https://github.com/dram1008/library",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "cs\\": "vendor/dram1008/library/lib"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
